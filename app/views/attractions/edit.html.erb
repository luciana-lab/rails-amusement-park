<h1>Editing attraction</h1>

<%= form_for @attraction do |f| %>
    <%= render partial: 'form', locals: { :f => f } %>
    
    <div class="actions">
    <%= f.submit "Update Attraction", class: "btn btn-primary" %>
    </div>
<% end %>

<%= link_to "Show", attraction_path(@attraction)%> | 
<%= link_to "Back", attractions_path %>